				"paragraphStyle" : {
					"__type" : "textStyle"
				},
				"string" : "###Leibniz is less than a programming language"
			},
			{
				"__type" : "textSnippet",
				"children" : {
					"__type" : "snippets",
					"items" : [ ]
				},
				"createEmail" : {
